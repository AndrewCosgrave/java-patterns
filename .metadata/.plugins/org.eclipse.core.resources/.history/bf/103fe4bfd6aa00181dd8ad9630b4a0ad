package adapter;

public class EmployeeLdap {
	private int cn ; 
	private String surname ; 
	private String givenName ;
	private String mail; 
	
	public EmployeeLdap(int cn , String mail , String givenName , String surname) {
		setId(cn);
		setEmail(mail); 
		setFirstName(givenName);
		setSecondName(surname);
	}
}
